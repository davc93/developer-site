{
  "name": "public-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run migrations:run && tsc && vite build",
    "preview": "vite preview",
    "lint:api": "eslint --config .eslintrc.json api",
    "lint:web": "eslint --config .eslintrc.cjs src",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:status": "sequelize-cli db:migrate:status",
    "migrations:run": "sequelize-cli db:migrate"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vercel/node": "^3.0.11",
    "eslint": "^8.54.0",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "marked": "^9.0.3",
    "mongoose": "^7.5.0",
    "motion": "^10.16.2",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1"
  }
}
