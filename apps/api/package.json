{
  "name": "api",
  "private": true,
  "version": "1.0.0",
  "description": "Api for the backend",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --inspect ./src/index.js",
    "start": "npm run migrations:run && node ./src/index.js",
    "user:create": "node ./scripts/createAdmin.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:status": "sequelize-cli db:migrate:status",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "backup:all": "node ./scripts/backup.js",
    "backup:schemas": "node ./scripts/backup-schemas.js",
    "restore": "node ./scripts/restore.js"
  },
  "keywords": [
    "nodejs",
    "firebase"
  ],
  "author": "Diego Vergara <davc93@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@graphql-tools/load-files": "^6.6.1",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.6.0",
    "joi": "^17.12.3",
    "jose": "^5.2.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "sequelize": "^6.37.2"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  }
}
